{
  "name": "@xrmzy/proto",
  "version": "1.0.0",
  "description": "HRIS proto definitions (Dynamic Loading)",
  "main": "index.js",
  "scripts": {
     "gen-node": "protoc -I ./proto --js_out=import_style=commonjs,binary:../.. --grpc_out=grpc_js:../.."
  },
  "files": [
  "**/*.proto",
  "index.js",
  "Makefile"
],
  "keywords": [
    "grpc",
    "proto",
    "hris",
    "protobuf"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@grpc/grpc-js": "^1.14.2",
    "@grpc/proto-loader": "^0.8.0",
    "google-protobuf": "^4.0.1",
    "protobufjs": "^7.5.4"
  },
  "devDependencies": {
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "grpc-tools": "^1.11.2",
    "protoc-gen-js": "^3.21.4-4",
    "ts-proto": "^2.8.3"
  }
}
